{
	"name": "Wee",
	"description": "Wee documentation and resources.",
	"config": {
		"watch": true,
		"minify": true,
		"enhanceTypography": true,
		"removeIndex": true,
		"removeTrailingSlashes": true,
		"paths": {
			"partials": "partials",
			"templates": "templates",
			"extensions": "extensions"
		}
	},
	"env": {
		"prod": {
			"domain": "https://www.weepower.com",
			"cdnUrl": "https://cdn.weepower.com",
			"assetUrl": "https://cdn.weepower.com/assets"
		},
		"default": {
			"domain": "http://www.weepower.dev",
			"cdnUrl": "",
			"assetUrl": "/assets"
		}
	},
	"data": {
		"version": "2.3.0",
		"analyticsID": "UA-40595891-1",
		"gitHubRoot": "https://github.com/weepower/wee/tree/master",
		"social": {
			"twitter": "weecss",
			"googlePageID": "+weecss"
		},
		"cssVersion": 1,
		"cssLegacyVersion": 1,
		"jsVersion": 2,
		"jsLegacyVersion": 1
	},
	"sections": {
		"home": {
			"name": "Homepage",
			"data": {
				"excludeNav": true
			},
			"template": "index",
			"target": "public_html/index.html",
			"content": [
				"content/pages/home.md"
			]
		},
		"404": {
			"name": "Page Not Found",
			"data": {
				"excludeNav": true
			},
			"template": "404",
			"target": "public_html/404.html",
			"content": "content/pages/404.md"
		},
		"blog": {
			"name": "Blog",
			"data": {
				"excludeNav": true,
				"seoTitle": "Development Blog",
				"seoDesc": "Stay up to date with the latest Wee news and developments."
			},
			"template": "blog",
			"target": [
				"public_html/blog/index.html"
			],
			"order": "created",
			"sort": "desc",
			"content": "content/blog/*.md",
			"sections": {
				"entries": {
					"name": "Entries",
					"data": {
						"excludeNav": true
					},
					"template": "entry",
					"target": [
						"public_html/blog/{{sourceName|slug}}.html"
					],
					"content": "content/blog/*.md"
				}
			}
		},
		"start": {
			"name": "Start",
			"template": "docs",
			"target": "public_html/start/index.html",
			"content": [
				"content/start/intro.md",
				"content/start/features.md",
				"content/start/installation.md",
				"content/start/compatibility.md",
				"content/start/bugs.md",
				"content/start/versioning.md",
				"content/start/community.md",
				"content/start/license.md"
			],
			"sections": {
				"structure": {
					"name": "Structure",
					"template": "docs",
					"target": "public_html/start/structure.html",
					"content": [
						"content/start/structure/intro.md",
						"content/start/structure/files.md",
						"content/start/structure/*.md"
					]
				},
				"markup": {
					"name": "Markup",
					"template": "docs",
					"target": "public_html/start/markup.html",
					"content": [
						"content/start/markup/intro.md",
						"content/start/markup/html5.md",
						"content/start/markup/doctype.md",
						"content/start/markup/meta.md",
						"content/start/markup/icons.md",
						"content/start/markup/publisher.md",
						"content/start/markup/twitter.md",
						"content/start/markup/opengraph.md",
						"content/start/markup/legacy.md",
						"content/start/markup/*.md"
					]
				}
			}
		},
		"build": {
			"name": "Build",
			"template": "docs",
			"target": "public_html/build/index.html",
			"content": [
				"content/build/intro.md",
				"content/build/setup.md",
				"content/build/tasks.md",
				"content/build/*.md",
				"!content/build/faq.md",
				"content/build/faq.md"
			],
			"sections": {
				"config": {
					"name": "Config",
					"template": "docs",
					"target": "public_html/build/config.html",
					"content": [
						"content/build/config/intro.md",
						"content/build/config/general.md",
						"content/build/config/style.md",
						"content/build/config/script.md",
						"content/build/config/generator.md",
						"content/build/config/server.md"
					]
				},
				"validation": {
					"name": "Validation",
					"template": "docs",
					"target": "public_html/build/validation.html",
					"content": [
						"content/build/validation/intro.md",
						"content/build/validation/task.md",
						"content/build/validation/*.md"
					]
				}
			}
		},
		"generator": {
			"name": "Generate",
			"template": "docs",
			"target": "public_html/generator/index.html",
			"content": [
				"content/generator/intro.md",
				"content/generator/*.md"
			],
			"sections": {
				"config": {
					"name": "Config",
					"template": "docs",
					"target": "public_html/generator/config.html",
					"content": [
						"content/generator/config/intro.md",
						"content/generator/config/general.md",
						"content/generator/config/config.md",
						"content/generator/config/env.md",
						"content/generator/config/data.md",
						"content/generator/config/*.md"
					]
				},
				"usage": {
					"name": "Usage",
					"template": "docs",
					"target": "public_html/generator/usage.html",
					"content": [
						"content/generator/usage/intro.md",
						"content/generator/usage/*.md"
					]
				}
			}
		},
		"style": {
			"name": "Style",
			"template": "docs",
			"target": "public_html/style/index.html",
			"content": [
				"content/style/intro.md",
				"content/style/reset.md",
				"content/style/print.md",
				"content/style/legacy.md"
			],
			"sections": {
				"variables": {
					"name": "Variables",
					"template": "docs",
					"target": "public_html/style/variables.html",
					"content": [
						"content/style/variables/intro.md",
						"content/style/variables/base.md",
						"content/style/variables/*.md"
					]
				},
				"mixins": {
					"name": "Mixins",
					"template": "docs",
					"target": "public_html/style/mixins.html",
					"content": [
						"content/style/mixins/intro.md",
						"content/style/mixins/*.md",
						"!content/style/mixins/other.md",
						"content/style/mixins/other.md"
					]
				},
				"classes": {
					"name": "Classes",
					"template": "docs",
					"target": "public_html/style/classes.html",
					"content": [
						"content/style/classes/intro.md",
						"content/style/classes/*.md"
					]
				},
				"styling": {
					"name": "Styling",
					"template": "docs",
					"target": "public_html/style/styling.html",
					"content": [
						"content/style/styling/intro.md",
						"content/style/styling/*.md"
					]
				}
			}
		},
		"script": {
			"name": "Script",
			"template": "docs",
			"target": "public_html/script/index.html",
			"content": [
				"content/script/intro.md",
				"content/script/*.md"
			],
			"sections": {
				"core": {
					"name": "Core",
					"template": "docs",
					"target": "public_html/script/core.html",
					"content": [
						"content/script/core/intro.md",
						"content/script/core/fn.make.md",
						"content/script/core/fn.extend.md",
						"content/script/core/*.md"
					]
				},
				"chain": {
					"name": "Chain",
					"template": "docs",
					"target": "public_html/script/chain.html",
					"content": [
						"content/script/chain/intro.md",
						"content/script/chain/register.md",
						"content/script/chain/core.md",
						"content/script/chain/*.md"
					]
				},
				"assets": {
					"name": "Assets",
					"template": "docs",
					"target": "public_html/script/assets.html",
					"content": [
						"content/script/assets/intro.md",
						"content/script/assets/*.md"
					]
				},
				"data": {
					"name": "Data",
					"template": "docs",
					"target": "public_html/script/data.html",
					"content": [
						"content/script/data/intro.md",
						"content/script/data/*.md"
					]
				},
				"dom": {
					"name": "DOM",
					"template": "docs",
					"target": "public_html/script/dom.html",
					"content": [
						"content/script/dom/intro.md",
						"content/script/dom/*.md"
					]
				},
				"events": {
					"name": "Events",
					"template": "docs",
					"target": "public_html/script/events.html",
					"content": [
						"content/script/events/intro.md",
						"content/script/events/*.md"
					]
				},
				"routes": {
					"name": "Routes",
					"template": "docs",
					"target": "public_html/script/routes.html",
					"content": [
						"content/script/routes/intro.md",
