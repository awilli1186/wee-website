					"content": [
						"content/script/screen/intro.md",
						"content/script/screen/values.md",
						"content/script/screen/*.md"
					]
				},
				"view": {
					"name": "View",
					"template": "docs",
					"target": "public_html/script/view.html",
					"content": [
						"content/script/view/intro.md",
						"content/script/view/render.md",
