{
	"name": "Documentation",
	"config": {
		"watch": true,
		"minify": true,
		"enhanceTypography": true,
		"removeIndex": true,
		"removeTrailingSlashes": false,
		"defaultTemplate": "docs",
		"paths": {
			"content": "content",
			"target": "../public_html",
			"partials": "partials",
			"templates": "templates",
			"helpers": "helpers"
		}
	},
	"env": {
		"default": {
			"domain": "http://www.weepower.dev",
			"cdnUrl": "",
			"assetUrl": "/assets"
		},
		"prod": {
			"domain": "https://www.weepower.com",
			"cdnUrl": "https://cdn.weepower.com",
			"assetUrl": "https://cdn.weepower.com/assets"
		}
	},
	"data": {
		"version": "3.0.0",
		"analyticsID": "UA-40595891-1",
		"gitHubRoot": "https://github.com/weepower/wee/tree/master",
		"social": {
			"twitter": "weecss",
			"googlePageID": "+weecss"
		},
		"cssVersion": 5,
		"cssLegacyVersion": 2,
		"jsVersion": 3,
		"jsLegacyVersion": 1
	},
	"sections": {
		"sitemap": {
			"name": "Sitemap",
			"data": {
				"excludeNav": true,
				"excludeSitemap": true
			},
			"template": "sitemap.xml",
			"target": "sitemap.xml",
			"content": "blog/*.md"
		},
		"search": {
			"name": "Search",
			"data": {
				"excludeNav": true,
				"excludeSitemap": true
			},
			"template": "search.json",
			"target": "search.json",
			"content": [
				"**/*.md",
				"!blog/*.md",
				"!pages/*.md"
			]
		},
		"home": {
			"name": "Homepage",
			"data": {
				"excludeNav": true,
				"excludeSitemap": true
			},
			"template": "index",
			"target": "index.html",
			"content": [
				"pages/home.md"
			]
		},
		"404": {
			"name": "Page Not Found",
			"data": {
				"excludeNav": true,
				"excludeSitemap": true
			},
			"template": "404",
			"target": "404.html",
			"content": "pages/404.md"
		},
		"blog": {
			"name": "Blog",
			"data": {
				"excludeNav": true,
				"seoTitle": "Development Blog",
				"seoDesc": "Stay up to date with the latest Wee news and development."
			},
			"template": "blog",
			"target": [
				"blog/index.html"
			],
			"order": "created",
			"sort": "desc",
			"content": "blog/*.md",
			"sections": {
				"entries": {
					"name": "Entries",
					"data": {
						"excludeNav": true,
						"excludeSitemap": true
					},
					"template": "entry",
					"target": [
						"blog/{{ sourceName|slug }}.html"
					],
					"content": "blog/*.md"
				}
			}
		},
		"start": {
			"name": "Start",
			"target": "start/index.html",
			"contentRoot": "v3/start",
			"content": [
				"intro.md",
				"features.md",
				"compatibility.md",
				"bugs.md",
				"versioning.md",
				"community.md",
				"license.md"
			],
			"sections": {
				"installation": {
					"name": "Installation",
					"target": "start/installation.html",
					"contentRoot": "v3/start/installation",
					"content": [
						"intro.md",
						"*.md"
					]
				},
				"structure": {
					"name": "Structure",
					"target": "start/structure.html",
					"contentRoot": "v3/start/structure",
					"content": [
						"intro.md",
						"files.md",
						"*.md"
					]
				},
				"markup": {
					"name": "Markup",
					"target": "start/markup.html",
					"contentRoot": "v3/start/markup",
					"content": [
						"intro.md",
						"html5.md",
						"doctype.md",
						"meta.md",
						"icons.md",
						"publisher.md",
						"twitter.md",
						"opengraph.md",
						"legacy.md",
						"*.md"
					]
				}
			}
		},
		"build": {
			"name": "Build",
			"target": "build/index.html",
			"contentRoot": "v3/build",
			"content": [
				"intro.md",
				"tasks.md",
				"*.md"
			],
			"sections": {
				"config": {
					"name": "Config",
					"target": "build/config.html",
					"contentRoot": "v3/build/config",
					"content": [
						"intro.md",
						"general.md",
						"style.md",
						"script.md",
						"generator.md",
						"server.md"
					]
				},
				"validation": {
					"name": "Validation",
					"target": "build/validation.html",
					"contentRoot": "v3/build/validation",
					"content": [
						"intro.md",
						"task.md",
						"*.md"
					]
				},
				"modules": {
					"name": "Modules",
					"target": "build/modules.html",
					"contentRoot": "v3/build/modules",
					"content": [
						"intro.md",
						"*.md"
					]
				}
			}
		},
		"generator": {
			"name": "Generate",
			"target": "generator/index.html",
			"contentRoot": "v3/generator",
			"content": [
				"intro.md",
				"*.md"
			],
			"sections": {
				"config": {
					"name": "Config",
					"target": "generator/config.html",
					"contentRoot": "v3/generator/config",
					"content": [
						"intro.md",
						"general.md",
						"config.md",
						"env.md",
						"data.md",
						"*.md"
					]
				},
				"usage": {
					"name": "Usage",
					"target": "generator/usage.html",
					"contentRoot": "v3/generator/usage",
					"content": [
						"intro.md",
						"*.md"
					]
				}
			}
		},
		"style": {
			"name": "Style",
			"target": "style/index.html",
			"contentRoot": "v3/style",
			"content": [
				"intro.md",
				"reset.md",
				"print.md",
				"legacy.md"
			],
			"sections": {
				"variables": {
					"name": "Variables",
					"target": "style/variables.html",
					"contentRoot": "v3/style/variables",
					"content": [
						"intro.md",
						"base.md",
						"*.md"
					]
				},
				"mixins": {
					"name": "Mixins",
					"target": "style/mixins.html",
					"contentRoot": "v3/style/mixins",
					"content": [
						"intro.md",
						"*.md",
						"!other.md",
