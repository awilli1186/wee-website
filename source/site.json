{
	"name": "Documentation",
	"config": {
		"watch": true,
		"minify": true,
		"enhanceTypography": true,
		"removeIndex": true,
		"removeTrailingSlashes": false,
		"paths": {
			"content": "content",
			"target": "../public_html",
			"partials": "partials",
			"templates": "templates",
			"helpers": "helpers"
		}
	},
	"env": {
		"default": {
			"domain": "http://www.weepower.dev",
			"cdnUrl": "",
			"assetUrl": "/assets"
		},
		"prod": {
			"domain": "https://www.weepower.com",
			"cdnUrl": "https://cdn.weepower.com",
			"assetUrl": "https://cdn.weepower.com/assets"
		}
	},
	"data": {
		"version": "2.4.1",
		"analyticsID": "UA-40595891-1",
		"gitHubRoot": "https://github.com/weepower/wee/tree/master",
		"social": {
			"twitter": "weecss",
			"googlePageID": "+weecss"
		},
		"cssVersion": 5,
		"cssLegacyVersion": 2,
		"jsVersion": 3,
		"jsLegacyVersion": 1
	},
	"sections": {
		"sitemap": {
			"name": "Sitemap",
			"data": {
				"excludeNav": true,
				"excludeSitemap": true
			},
			"template": "sitemap.xml",
			"target": "sitemap.xml",
			"content": "blog/*.md"
		},
		"search": {
			"name": "Search",
			"data": {
				"excludeNav": true,
				"excludeSitemap": true
			},
			"template": "search.json",
			"target": "search.json",
			"content": [
				"**/*.md",
				"!blog/*.md",
				"!pages/*.md"
			]
		},
		"home": {
			"name": "Homepage",
			"data": {
				"excludeNav": true,
				"excludeSitemap": true
			},
			"template": "index",
			"target": "index.html",
			"content": [
				"pages/home.md"
			]
		},
		"404": {
			"name": "Page Not Found",
			"data": {
				"excludeNav": true,
				"excludeSitemap": true
			},
			"template": "404",
			"target": "404.html",
			"content": "pages/404.md"
		},
		"blog": {
			"name": "Blog",
			"data": {
				"excludeNav": true,
				"seoTitle": "Development Blog",
				"seoDesc": "Stay up to date with the latest Wee news and developments."
			},
			"template": "blog",
			"target": [
				"blog/index.html"
			],
			"order": "created",
			"sort": "desc",
			"content": "blog/*.md",
			"sections": {
				"entries": {
					"name": "Entries",
					"data": {
						"excludeNav": true,
						"excludeSitemap": true
					},
					"template": "entry",
					"target": [
						"blog/{{sourceName|slug}}.html"
					],
					"content": "blog/*.md"
				}
			}
		},
		"start": {
			"name": "Start",
			"template": "docs",
			"target": "start/index.html",
			"contentRoot": "v3",
			"content": [
				"start/intro.md",
				"start/features.md",
				"start/compatibility.md",
				"start/bugs.md",
				"start/versioning.md",
				"start/community.md",
				"start/license.md"
			],
			"sections": {
				"installation": {
					"name": "Installation",
					"template": "docs",
					"target": "start/installation.html",
					"contentRoot": "v3",
					"content": [
						"start/installation/intro.md",
						"start/installation/*.md"
					]
				},
				"structure": {
					"name": "Structure",
					"template": "docs",
					"target": "start/structure.html",
					"contentRoot": "v3",
					"content": [
						"start/structure/intro.md",
						"start/structure/files.md",
						"start/structure/*.md"
					]
				},
				"markup": {
					"name": "Markup",
					"template": "docs",
					"target": "start/markup.html",
					"contentRoot": "v3",
					"content": [
						"start/markup/intro.md",
						"start/markup/html5.md",
						"start/markup/doctype.md",
						"start/markup/meta.md",
						"start/markup/icons.md",
						"start/markup/publisher.md",
						"start/markup/twitter.md",
						"start/markup/opengraph.md",
						"start/markup/legacy.md",
						"start/markup/*.md"
					]
				}
			}
		},
		"build": {
			"name": "Build",
			"template": "docs",
			"target": "build/index.html",
			"contentRoot": "v3",
			"content": [
				"build/intro.md",
				"build/setup.md",
				"build/tasks.md",
				"build/*.md",
				"!build/faq.md",
				"build/faq.md"
			],
			"sections": {
				"config": {
					"name": "Config",
					"template": "docs",
					"target": "build/config.html",
					"contentRoot": "v3",
					"content": [
						"build/config/intro.md",
						"build/config/general.md",
						"build/config/style.md",
						"build/config/script.md",
						"build/config/generator.md",
						"build/config/server.md"
					]
				},
				"validation": {
					"name": "Validation",
					"template": "docs",
					"target": "build/validation.html",
					"contentRoot": "v3",
					"content": [
						"build/validation/intro.md",
						"build/validation/task.md",
						"build/validation/*.md"
					]
				},
				"modules": {
					"name": "Modules",
					"template": "docs",
					"target": "build/modules.html",
					"contentRoot": "v3",
					"content": [
						"build/modules/*.md"
					]
				}
			}
		},
		"generator": {
			"name": "Generate",
			"template": "docs",
			"target": "generator/index.html",
			"contentRoot": "v3",
			"content": [
				"generator/intro.md",
				"generator/*.md"
			],
			"sections": {
				"config": {
					"name": "Config",
					"template": "docs",
					"target": "generator/config.html",
					"contentRoot": "v3",
					"content": [
						"generator/config/intro.md",
						"generator/config/general.md",
						"generator/config/config.md",
						"generator/config/env.md",
						"generator/config/data.md",
						"generator/config/*.md"
					]
				},
				"usage": {
					"name": "Usage",
					"template": "docs",
					"target": "generator/usage.html",
					"contentRoot": "v3",
					"content": [
						"generator/usage/intro.md",
						"generator/usage/*.md"
					]
